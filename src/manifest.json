{
  "name": "todo-list",
  "description": "一个管理任务清单的谷歌插件。",
  "version": "1.1.0",
  "manifest_version": 3,
  "icons": {
    "48": "icons/todo_48.png",
    "128": "icons/todo_128.png"
  },
  "action": {
    "default_title": "todo-list",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions":["storage", "unlimitedStorage"]
}
